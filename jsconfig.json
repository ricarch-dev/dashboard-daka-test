{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": false,
    "strict": false,
    "noEmit": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "vueCompilerOptions": {
    "target": 3
  },
  "include": ["src/**/*", "*.vue"],
  "exclude": ["node_modules", "dist"]
}
